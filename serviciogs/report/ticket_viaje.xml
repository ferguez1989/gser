<?xml version="1.0" encoding="utf-8" ?>
<odoo>
  <template id="report_ticket_viaje">
    <t t-call="web.html_container">
        <t t-foreach="docs" t-as="o">
            <t t-call="web.external_layout">
                <div class="page">
                  <div class="col-sm">
                    <table class="table table-striped">
                      <thead>
                        <tr>
                          <th scope="col">#</th>
                          <th scope="col">First</th>
                          <th scope="col">Last</th>
                          <th scope="col">Handle</th>
                        </tr>
                      </thead>
                      <tbody>
                        <tr>
                          <th scope="row">1</th>
                          <td>Mark</td>
                          <td>Otto</td>
                          <td>@mdo</td>
                        </tr>
                        <tr>
                          <th scope="row">2</th>
                          <td>Jacob</td>
                          <td>Thornton</td>
                          <td>@fat</td>
                        </tr>
                        
                      </tbody>
                    </table>
                  </div>
                    <h2>Report title</h2>
                    <p>This object's name is <span t-field="o.company_id"/></p>
                </div>
            </t>
        </t>
    </t>
  </template>
</odoo>